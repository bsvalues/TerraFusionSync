{
  "name": "marketplace-sync",
  "version": "0.1.0",
  "description": "Marketplace data synchronization plugin",
  "routes": [
    {
      "path": "/marketplace/sync",
      "component": "SyncDashboard",
      "exact": true,
      "name": "Sync Dashboard",
      "menuItem": true,
      "menuGroup": "Marketplace",
      "menuIcon": "sync-alt"
    },
    {
      "path": "/marketplace/sync/new",
      "component": "NewSyncWizard",
      "exact": true,
      "name": "New Sync",
      "menuItem": false
    },
    {
      "path": "/marketplace/sync/:id",
      "component": "SyncDetails",
      "exact": true,
      "name": "Sync Details",
      "menuItem": false
    }
  ]
}